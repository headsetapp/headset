{
  "name": "headset",
  "productName": "Headset",
  "version": "1.6.0",
  "description": "Discover and collect music on YouTube",
  "license": "MIT",
  "author": "Daniel Ravina <hello@headsetapp.co>",
  "homepage": "http://headsetapp.co",
  "bugs": "https://github.com/headsetapp/headset-electron/issues",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development nf start",
    "electron": "electron .",
    "player": "./bin/player-window.sh",
    "production": "NODE_ENV=production electron .",
    "pack": "NODE_ENV=production NEW_VERSION=$npm_package_version ./bin/build_package.sh",
    "dist": "NODE_ENV=production NEW_VERSION=$npm_package_version ./bin/build_installers.sh",
    "build": "npm run pack && npm run dist",
    "postinstall": "electron-rebuild && ./bin/path.sh",
    "test": "npm run pack && OS=linux mocha ../test/app.js && npm run dist && mocha test/installers.js"
  },
  "dependencies": {
    "dbus": "^0.2.23",
    "electron-window-state": "^4.1.1",
    "mpris-service": "^1.1.3"
  },
  "devDependencies": {
    "electron-installer-debian": "fcastilloec/electron-installer-debian#lintian",
    "electron-installer-redhat": "^0.5.0",
    "electron-rebuild": "^1.6.0"
  }
}
