{
  "name": "headset",
  "productName": "Headset",
  "version": "1.6.0",
  "description": "Discover and collect music on YouTube",
  "license": "MIT",
  "author": "Daniel Ravina",
  "homepage": "http://headsetapp.co",
  "bugs": "https://github.com/headsetapp/headset-electron/issues",
  "main": "index.js",
  "scripts": {
    "start": "nf start",
    "electron": "electron .",
    "production": "NODE_ENV=production electron .",
    "build": "electron-packager ./ Headset --overwrite --platform=win32 --asar --arch=ia32 --ignore=node_modules/electron-prebuilt --ignore=node_modules/electron-packager --out=build --icon=./Headset.ico --version-string.ProductName=\"Headset\"",
    "installer": "node bin/winstaller.js",
    "ship": "npm run build && npm run installer"
  },
  "dependencies": {
    "electron-positioner": "^3.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-window-state": "^4.1.1"
  },
  "devDependencies": {
    "electron": "^1.7.8",
    "electron-packager": "^9.1.0",
    "electron-winstaller": "^2.6.3",
    "foreman": "^2.0.0",
    "http-server": "^0.10.0",
    "mocha": "^3.5.3",
    "spectron": "^3.7.2"
  }
}
