#!/usr/bin/env bash

electron-packager . \
  --ignore=node_modules/electron-prebuilt \
  --ignore=node_modules/electron-packager \
  --out build/ \
  --overwrite \
  --platform=linux \
  --arch=x64

electron-installer-debian \
  --src build/Headset-linux-x64/ \
  --dest build/installers/ \
  --arch amd64 \
  --config config.json
